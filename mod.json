		"functions":{
			"name": "Functionality",
			"description": "How the mod functions.",
			"type": "title"
		},
		"confirm-use":{
			"name": "Confirm Actions",
			"description": "Add a confirmation popup when using an editor action.",
			"type": "bool",
			"default": true
		}
	},
	"links": {
