{
	"geode": "3.8.1",
	"gd": {
		"win": "2.206",
		"android": "2.206",
		"mac": "2.206",
		"ios": "2.206"
	},
	"id": "arcticwoof.bettereditorpause",
	"name": "BetterEditorPause",
	"version": "v1.0.0",
	"developers": [
		"ArcticWoof",
		"Cheeseworks"
	],
	"description": "Organizes editor pause menu's tools.",
	"tags": [
		"editor",
		"interface",
		"enhancement"
	],
	"resources": {
		"sprites": [
			"resources/*.png"
		]
	},
	"dependencies": [
		{
			"id": "geode.node-ids",
			"version": ">=1.15.0",
			"importance": "required"
		}
	],
	"settings": {
		"ui": {
			"name": "Interface",
			"description": "The mod's user interface.",
			"type": "title"
		},
		"move-tools": {
			"name": "Move Creator Actions",
			"description": "Organize the editor pause menu's creator action buttons.",
			"type": "bool",
			"default": true
		},
		"move-toggles": {
			"name": "Move Pause Options",
			"description": "Organize the editor pause menu's editor options.",
			"type": "bool",
			"default": true
		},
		"move-resume": {
			"name": "Move Resume Menu",
			"description": "Organize the editor pause menu's resume menu.",
			"type": "bool",
			"default": true
		},
		"move-info": {
			"name": "Move Editor Information",
			"description": "Organize the editor pause menu's information labels.",
			"type": "bool",
			"default": true
		}
	},
	"links": {
		"community": "https://www.discord.com/invite/2jPCtJhHNp",
		"homepage": "https://www.arcticwoof.com.au/",
		"source": "https://github.com/DumbCaveSpider/BetterEditorPause/"
	},
	"api": {
		"include": [
			"src/util/*.hpp"
		]
	}
}